#include <bits/stdc++.h>
using namespace std;

int main() {
	string s;
	cin>>s;
	long long int i,j,k=0;
	
	for(i=0;i<s.length();i++)
	{
	    if(s[i]=='1')
	    break;
	}
	for(;i<s.length();i++)
	{
	    if(s[i]=='0')
	    k++;
	}
	if(k>=6)
	cout<<"yes\n";
	else
	cout<<"no\n";
	return 0;
}
